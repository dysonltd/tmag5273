        let config = Config::default();
        let p = embassy_stm32::init(config);
        let i2c = I2c::new(
            p.I2C1,
            p.PB8,
            p.PB9,
            Irqs,
            NoDma,
            NoDma,
            Hertz(400_000),
            Default::default(),
        );
        i2c
    }
